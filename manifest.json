{
	"name": "自动刷课",
	"version": "0.0.3",
	"description": "由李大炮编写的自动刷课插件",
	"manifest_version": 3,
	"content_scripts": [
	  {
		"matches": [
		  "https://hnwledu.ls365.net/*",
		  "https://huse.ls365.net/*",
		  "https://bx.bossyun.com/*"
		],
		"css": [],
		"js": [
		  "static/js/jquery.js",
		  "static/js/init.js",
		  "static/js/content-script.js"
		],
		"run_at": "document_start"
	  }
	],
	"permissions": [
	  "contextMenus",
	  "tabs",
	  "notifications",
	  "webRequest",
	  "storage"
	],
	"action": {
	  "default_popup": "index.html",
	  "default_icon": {
		"16": "static/img/icon.png",
		"32": "static/img/icon.png",
		"48": "static/img/icon.png",
		"128": "static/img/icon.png"
	  },
	  "default_title": "李大炮祝您今天愉快✿"
	},
	"icons": {
	  "16": "static/img/icon.png",
	  "32": "static/img/icon.png",
	  "48": "static/img/icon.png",
	  "128": "static/img/icon.png"
	}
  }
  